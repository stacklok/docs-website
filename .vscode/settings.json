{
  "[markdown][mdx][css][json][jsonc][yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "css.lint.emptyRules": "ignore",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.enable": true,
  "eslint.format.enable": false,
  "markdown.validate.enabled": true,
  "markdown.validate.fileLinks.enabled": "warning",
  "markdown.validate.fileLinks.markdownFragmentLinks": "warning",
  "markdown.validate.fragmentLinks.enabled": "warning",
  "markdown.validate.referenceLinks.enabled": "ignore",
  "markdown.validate.unusedLinkDefinitions.enabled": "hint",
  "mdx.server.enable": true,
  "mdx.validate.validateFileLinks": "warning",
  "mdx.validate.validateFragmentLinks": "warning",
  "mdx.validate.validateMarkdownFileLinkFragments": "warning",
  "mdx.validate.validateReferences": "ignore",
  "mdx.validate.validateUnusedLinkDefinitions": "hint",
  "prettier.configPath": ".prettierrc",
  "prettier.enable": true,
  "prettier.ignorePath": ".prettierignore",
  "prettier.requireConfig": true,
  "yaml.format.proseWrap": "always"
}
